{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqlServerString": "Server=localhost,1433;Database=EcommerceApp;TrustServerCertificate=True;"
  },
  "JWTOptions": {
    "SecretKey": "random-key-is-here",
    "validIssuer": "Ecommerce-Api",
    "validAudience": "Ecommerce-Api-Client",
    "expires": 60
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": false,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "IpWhitelist": [],
    "EndpointWhitelist": ["get:/api/whaterehere"],
    "ClientWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 25
      }
    ],
    "IpRateLimitPolicies": {
      "IpRules": [
        {
          "Ip": "127.0.0.1",
          "Rules": [
            {
              "Endpoint": "*",
              "Period": "1s",
              "Limit": 10
            }
          ]
        }
      ]
    }
  }
}
